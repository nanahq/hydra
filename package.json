  "workspace": [
    "apps/*",
    "libs/*"
  ],
  "engines": {
    "node": ">=16.16",
    "npm": ">=8.11"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "ci:test": "jest --ci --coverage --forceExit --maxWorkers=4",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/eatlater/test/jest-e2e.json",
    "all:build": "npm run build users-service && npm run build nofication-service && npm run build vendors-service && npm run build admin-service && npm run build listings-service",
    "all:version": "lerna version $1 --yes --no-push --no-git-tag-version --exact"
  },
  "dependencies": {
    "@nestjs/common": "^9.1.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.6",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "^9.1.4",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.4",
    "@nestjs/throttler": "^3.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "amqp-connection-manager": "^4.1.6",
    "amqplib": "^0.10.3",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "helmet": "^6.0.0",
    "i": "^0.3.7",
    "joi": "^17.6.2",
    "nanoid": "^4.0.0",
    "nestjs-twilio": "^3.0.0",
    "npm": "^8.19.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
