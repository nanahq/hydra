{
  "name": "nana",
  "version": "1.49.0",
  "description": "",
  "author": "NanaEats Logistics",
  "private": true,
  "license": "UNLICENSED",
  "workspace": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/nana/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "ci:test": "jest --ci --coverage --forceExit --maxWorkers=4",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/eatlater/test/jest-e2e.json",
    "all:build": "yarn build users-service && yarn build nofication-service && yarn build vendors-service && yarn build admin-service && yarn build listings-service && yarn build payment-service  && yarn build reviews-service && yarn build vendors-gateway && yarn build admin-gateway && yarn build api-gateway && yarn build orders-service && yarn build drivers-service && yarn build location-service",
    "all:version": "lerna version $1 --yes --no-push --no-git-tag-version --exact",
    "doc": "npx compodoc -p tsconfig.json -n \"EatLater Docs\"",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org nana-hq --project nana-hydra ./dist && sentry-cli sourcemaps upload --org nana-hq --project nana-hydra ./dist"
  },
  "dependencies": {
