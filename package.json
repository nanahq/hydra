  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "ci:test": "jest --ci --coverage --forceExit --maxWorkers=4",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/eatlater/test/jest-e2e.json",
    "all:build": "npm run build users-service && npm run build nofication-service && npm run build vendors-service && npm run build admin-service && npm run build listings-service && npm run build payment-service",
    "all:version": "lerna version $1 --yes --no-push --no-git-tag-version --exact",
    "doc": "npx compodoc -p tsconfig.json -n \"EatLater Docs\""
  },
